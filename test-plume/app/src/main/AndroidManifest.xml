<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidfarmmonitor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidFarmMonitor">
        <activity android:name=".MarkerResourceActivity" />
        <activity android:name=".MarkerListActivity" />
        <activity android:name=".ResourceActivity" />
        <activity android:name=".MainNavigationActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".AddDataActivity" />
        <activity android:name=".FieldDetailActivity" />
        <activity android:name=".TaskActivity" />
        <activity android:name=".DataActivity" />
    </application>

</manifest>
